                                        Swiped="SwipeGestureRecognizer_Swiped3"/>
                <SwipeGestureRecognizer Direction="Right"
                                        Swiped="SwipeGestureRecognizer_Swiped3"/>
                <SwipeGestureRecognizer Direction="Up"
                                        Swiped="SwipeGestureRecognizer_Swiped3"/>
